<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yi.mapper.BadnessMapper">

	<resultMap type="BadnessVO" id="BadnessStandard">
		<result property="bNo" column="b_no" />
		<result property="bQy" column="b_qy" />
		<result property="bProcess" column="b_process" />
		<result property="bSetup" column="b_setup" />
		<result property="bMaterial" column="b_material" />
		<result property="bEtc" column="b_etc" />
	</resultMap>
	<resultMap type="BadnessVO" id="BadnessResultMap" extends="BadnessStandard">
		<association property="pNo" resultMap="com.yi.mapper.ProductMapper.ProductResultMap"/>
	</resultMap>
	
	<select id="selectByBadness">
		select * from badness
	</select>
	
	
	
	
	
	
	
</mapper>

















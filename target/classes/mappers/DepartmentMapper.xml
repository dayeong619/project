<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yi.mapper.DepartmentMapper">
	
	<resultMap type="DepartmentVO" id="DepartmentResultMap">
		<id property="dNo" column="d_no" />
		<result property="dName" column="d_name" />
	</resultMap>
	
	<insert id="insertDepartment">
		insert into department(d_name) values (#{dName}) 
	</insert>
	
	<select id="selectAllDepartment" resultType="DepartmentVO">
		select * from department
	</select>
	
	
	
	
	
	
	
	
	
	
	
	
</mapper>
















